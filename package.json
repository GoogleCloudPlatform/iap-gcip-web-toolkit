    "rollup": "1.4.0",
    "rollup-plugin-babel-minify": "^6.2.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-typescript2": "^0.12.0",
    "selenium-webdriver": "^4.0.0-alpha.4",
    "sinon": "^4.4.5",
    "sinon-chai": "^2.8.0",
    "ts-node": "^3.3.0",
