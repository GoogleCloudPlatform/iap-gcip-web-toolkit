{
  "name": "cicp-iap-js",
  "version": "0.0.0",
  "description": "CICP/IAP JS SDK",
  "author": "Google",
  "license": "Apache-2.0",
  "homepage": "https://team.git.corp.google.com/cicp-eng/cicp-iap-js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "gulp build && rollup -c",
    "lint": "run-p lint:src lint:unit lint:integration lint:typings",
    "test": "run-s lint test:browser:unit",
    "test:browser": "run-s test:browser:unit test:browser:integration",
    "test:browser:unit": "karma start karma.config.js --type=unit",
    "test:browser:integration": "karma start karma.config.js --type=integration",
    "lint:src": "tslint --format stylish -p tsconfig.json src/**/*.ts src/index.ts",
    "lint:unit": "tslint -c tslint-test.json --format stylish test/unit/*.ts test/unit/**/*.ts",
    "lint:integration": "tslint -c tslint-test.json --format stylish test/integration/*.ts",
    "lint:typings": "tslint -c tslint-types.json --format stylish src/index.d.ts"
  },
  "keywords": [
    "cloud",
    "identity",
    "cicp",
    "iap",
    "firebase",
    "proxy",
    "aware",
    "authentication"
  ],
  "repository": {
    "type": "git",
    "url": "https://team.git.corp.google.com/cicp-eng/cicp-iap-js"
  },
  "main": "dist/index.cjs.js",
  "browser": "dist/index.iife.js",
  "module": "dist/index.esm.js",
  "files": [
    "lib/",
    "dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "types": "./lib/index.d.ts",
  "dependencies": {
    "@types/node": "^8.0.53",
    "node-forge": "0.7.4"
  },
  "peerDependencies": {
    "@firebase/app": "^0.3.5",
    "@firebase/auth": "^0.9.1",
    "@firebase/auth-types": "^0.5.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/chai-as-promised": "0.0.29",
    "@types/lodash": "^4.14.104",
    "@types/mocha": "^2.2.48",
    "@types/nock": "^9.1.0",
    "@types/sinon": "^4.1.3",
    "@types/sinon-chai": "^2.7.27",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chalk": "^1.1.3",
    "del": "^2.2.1",
    "dtslint": "^0.4.1",
    "@firebase/app": "^0.3.5",
    "@firebase/auth": "^0.9.1",
    "@firebase/auth-types": "^0.5.0",
    "gulp": "^3.9.1",
    "gulp-exit": "0.0.2",
    "gulp-header": "^1.8.8",
    "gulp-replace": "^0.5.4",
    "gulp-tslint": "^6.0.2",
    "gulp-typescript": "^3.2.4",
    "karma": "3.1.4",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "2.0.0",
    "karma-coverage": "1.1.2",
    "karma-html-reporter": "0.2.7",
    "karma-mocha": "1.3.0",
    "karma-sinon": "1.0.5",
    "karma-summary-reporter": "1.5.2",
    "karma-typescript": "3.0.13",
    "karma-typescript-preprocessor": "0.4.0",
    "karma-verbose-reporter": "0.0.6",
    "lodash": "^4.17.5",
    "merge2": "^1.2.1",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "nock": "^9.1.8",
    "npm-run-all": "^4.1.2",
    "promise-polyfill": "^8.1.0",
    "rollup": "^0.57.1",
    "rollup-plugin-babel-minify": "^6.2.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-typescript2": "^0.12.0",
    "run-sequence": "^1.1.5",
    "sinon": "^4.4.5",
    "sinon-chai": "^2.8.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.9.0",
    "typescript": "^2.7.2",
    "url-polyfill": "^1.1.3",
    "whatwg-fetch": "^3.0.0",
    "webpack": "^3.0.0",
    "yargs": "12.0.5"
  }
}
