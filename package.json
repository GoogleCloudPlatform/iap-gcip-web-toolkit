{
  "name": "gcip-iap-js",
  "version": "0.0.4",
  "description": "GCIP/IAP JS SDK",
  "author": "Google",
  "license": "Apache-2.0",
  "homepage": "https://github.com/GoogleCloudPlatform/gcip-iap-js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "gulp build",
    "build-alpha": "npm run test:e2e && gulp build-alpha",
    "clean": "gulp cleanup",
    "rollup": "rollup -c",
    "lint": "run-p lint:src lint:unit lint:integration lint:typings",
    "test": "run-s lint test:unit",
    "test:unit": "karma start karma.config.js --type=unit",
    "test:integration": "mocha test/integration/index.spec.ts --slow 5000 --timeout 90000 --require ts-node/register",
    "test:e2e": "./build-authui.sh && npm run test:integration",
    "lint:src": "tslint --format stylish -p tsconfig.json src/**/*.ts src/index.ts",
    "lint:unit": "tslint -c tslint-test.json --format stylish test/unit/*.ts test/unit/**/*.ts test/resources/**/*.ts",
    "lint:integration": "tslint -c tslint-test.json --format stylish test/integration/*.ts",
    "lint:typings": "tslint -c tslint-types.json --format stylish src/index.d.ts"
  },
  "keywords": [
    "cloud",
    "identity",
    "platform",
    "gcip",
    "iap",
    "firebase",
    "proxy",
    "aware",
    "authentication"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/GoogleCloudPlatform/gcip-iap-js"
  },
  "main": "dist/index.cjs.js",
  "browser": "dist/index.iife.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@types/node": "^8.0.53",
    "node-forge": "0.7.4"
  },
  "peerDependencies": {
    "@firebase/app": "^0.3.5",
    "@firebase/auth": "^0.9.1",
    "@firebase/auth-types": "^0.5.0",
    "promise-polyfill": "^8.1.0",
    "url-polyfill": "^1.1.3",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@firebase/app": "^0.3.5",
    "@firebase/auth": "^0.9.1",
    "@firebase/auth-types": "^0.5.0",
    "@types/chai": "^3.4.34",
    "@types/chai-as-promised": "0.0.29",
    "@types/lodash": "^4.14.104",
    "@types/mocha": "^2.2.48",
    "@types/nock": "^9.1.0",
    "@types/sinon": "^4.1.3",
    "@types/sinon-chai": "^2.7.27",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chalk": "^1.1.3",
    "chromedriver": "latest",
    "del": "^2.2.1",
    "dtslint": "^0.4.1",
    "firebase-admin": "^8.1.0",
    "firebase-tools": "~7.0.2",
    "gulp": "^4.0.2",
    "gulp-exit": "0.0.2",
    "gulp-gzip": "^1.4.2",
    "gulp-header": "^1.8.8",
    "gulp-replace": "^0.5.4",
    "gulp-tar": "^2.1.0",
    "gulp-tslint": "^6.0.2",
    "gulp-typescript": "^5.0.1",
    "karma": "^4.1.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "2.0.0",
    "karma-coverage": "1.1.2",
    "karma-mocha": "1.3.0",
    "karma-sinon": "1.0.5",
    "karma-typescript": "4.0.0",
    "karma-typescript-es6-transform": "4.0.0",
